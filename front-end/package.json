{
  "name": "front-end",
  "description": "A Vue.js project",
  "version": "1.0.0",
  "author": "matheus carvalho",
  "private": true,
  "engines": {
    "node": ">=6",
    "npm": ">=3"
  },
  "scripts": {
    "clean": "rimraf dist && rimraf coverage",
    "dev": "cross-env NODE_ENV=development && npm run clean && webpack && webpack-dev-server",
    "prod": "cross-env NODE_ENV=production && npm run clean && webpack --config config/webpack.config.prod.js",
    "test": "cross-env NODE_ENV=development karma start karma.unit.js",
    "test:debug": "cross-env NODE_ENV=development karma start karma.debug.js",
    "test:watch": "cross-env NODE_ENV=development karma start karma.unit.js --singleRun=false --auto-watch",
    "coverage": "cross-env NODE_ENV=production && npm run clean && webpack --config config/webpack.config.coverage.js",
    "coverage:open": "opn coverage/html-ts/index.html",
    "coverage:remap": "remap-istanbul -i coverage/json/coverage-final.json -o coverage/html-ts -t html",
    "coverage:run": "cross-env NODE_ENV=development karma start karma.coverage.js",
    "lint": "tslint src/**/*.ts"
  },
  "dependencies": {
    "axios": "^0.15.3",
    "bootstrap": "^4.0.0-beta",
    "bootstrap-vue": "^0.18.0",
    "font-awesome": "^4.7.0",
    "instascan": "^1.0.0",
    "material-design-lite": "^1.3.0",
    "roboto-fontface": "^0.8.0",
    "vue": "~2.2.4",
    "vue-class-component": "~5.0.0",
    "vue-material": "^0.7.4",
    "vue-property-decorator": "~4.0.0",
    "vue-router": "~2.3.0",
    "vuex": "^2.3.1"
  },
  "devDependencies": {
    "@types/mocha": "~2.2.40",
    "@types/node": "~7.0.8",
    "@types/sinon": "~1.16.36",
    "awesome-typescript-loader": "~3.1.2",
    "babel-core": "^6.25.0",
    "babel-loader": "^7.1.1",
    "babel-preset-env": "^1.6.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-latest": "^6.24.1",
    "browser-sync": "^2.18.13",
    "browser-sync-webpack-plugin": "^1.2.0",
    "chai": "~3.5.0",
    "clean-css-cli": "~4.0.9",
    "compression-webpack-plugin": "^0.3.2",
    "concurrently": "~3.4.0",
    "copy-webpack-plugin": "~4.0.1",
    "cross-env": "~3.2.4",
    "css-hot-loader": "^1.3.0",
    "css-loader": "^0.28.4",
    "es6-promise": "~4.1.0",
    "extract-text-webpack-plugin": "^2.1.2",
    "file-loader": "~0.10.1",
    "globby": "~6.1.0",
    "htmlprocessor": "~0.2.4",
    "http-server": "~0.9.0",
    "istanbul-instrumenter-loader": "~2.0.0",
    "karma": "~1.5.0",
    "karma-chai": "~0.1.0",
    "karma-chrome-launcher": "~2.0.0",
    "karma-coverage": "~1.1.1",
    "karma-junit-reporter": "~1.2.0",
    "karma-mocha": "~1.3.0",
    "karma-mocha-reporter": "~2.2.2",
    "karma-phantomjs-launcher": "~1.0.4",
    "karma-sinon": "~1.0.5",
    "karma-source-map-support": "~1.2.0",
    "karma-sourcemap-loader": "~0.3.7",
    "karma-teamcity-reporter": "~1.0.0",
    "karma-webpack": "~2.0.3",
    "lodash.merge": "~4.6.0",
    "mocha": "~3.2.0",
    "node-sass": "~4.5.0",
    "raw-loader": "~0.5.1",
    "remap-istanbul": "~0.9.1",
    "rimraf": "~2.6.1",
    "sass-loader": "^6.0.6",
    "sinon": "~2.1.0",
    "style-loader": "^0.18.2",
    "ts-loader": "^2.3.2",
    "tslint": "~4.5.1",
    "tslint-loader": "~3.4.3",
    "typescript": "^2.4.2",
    "uglifyjs-webpack-plugin": "^0.4.6",
    "url-loader": "^0.5.9",
    "webpack": "^2.2.1",
    "webpack-dev-server": "^2.6.1"
  }
}
