{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "project": {
    "name": "front-end"
  },
  "apps": [{
    "root": "src",
    "outDir": "dist",
    "assets": [
      "assets",
      "favicon.ico"
    ],
    "index": "index.html",
    "main": "main.ts",
    "polyfills": "config/polyfills.ts",
    "test": "test.ts",
    "tsconfig": "config/ts/tsconfig.app.json",
    "testTsconfig": "config/ts/tsconfig.spec.json",
    "prefix": "app",
    "styles": [
      "sass/styles.scss",
      "../node_modules/bootstrap/dist/css/bootstrap.css",
      "../node_modules/font-awesome/css/font-awesome.css"
    ],
    "scripts": [],
    "environmentSource": "config/environments/environment.ts",
    "environments": {
      "dev": "config/environments/environment.ts",
      "prod": "config/environments/environment.prod.ts"
    }
  }],
  "e2e": {
    "protractor": {
      "config": "./protractor.conf.js"
    }
  },
  "lint": [{
      "project": "src/config/ts/tsconfig.app.json",
      "exclude": "**/node_modules/**"
    },
    {
      "project": "src/config/ts/tsconfig.spec.json",
      "exclude": "**/node_modules/**"
    },
    {
      "project": "e2e/tsconfig.e2e.json",
      "exclude": "**/node_modules/**"
    }
  ],
  "test": {
    "karma": {
      "config": "./karma.conf.js"
    }
  },
  "defaults": {
    "styleExt": "scss",
    "component": {}
  }
}
